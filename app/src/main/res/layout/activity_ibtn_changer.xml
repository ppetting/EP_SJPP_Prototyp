<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/g700"
    tools:context=".Menuebereich.IbtnChangerActivity">

    <include
        android:id="@+id/toolbarMAIN"
        layout="@layout/toolbar"
        app:layout_constraintEnd_toEndOf ="parent"
        app:layout_constraintStart_toStartOf = "parent"
        app:layout_constraintTop_toTopOf = "parent"
        />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_below="@id/toolbarMAIN">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <ImageButton
                android:id="@+id/imageButtonGruenerApfelChanger"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_centerHorizontal="true"
                android:scaleType="fitCenter"
                app:srcCompat="@drawable/gruenerapfel"
                android:backgroundTint="@color/g200"
                tools:ignore="NotSibling" />

            <TextView
                android:id="@+id/tvGruenerApfel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageButtonGruenerApfelChanger"
                android:text="gr체ner Apfel"
                android:textColor="@color/g200"
                android:layout_centerHorizontal="true"/>

            <ImageButton
                android:id="@+id/imageButtonRoterApfeleChanger"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_centerHorizontal="true"
                android:scaleType="fitCenter"
                android:layout_below="@id/tvGruenerApfel"
                app:srcCompat="@drawable/roterapfel"
                android:backgroundTint="@color/g200"
                tools:ignore="NotSibling" />

            <TextView
                android:id="@+id/tvRoterApfel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageButtonRoterApfeleChanger"
                android:text="roter Apfel"
                android:textColor="@color/g200"
                android:layout_centerHorizontal="true"/>

            <ImageButton
                android:id="@+id/imageButtonGurkeChanger"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_centerHorizontal="true"
                android:scaleType="fitCenter"
                android:layout_below="@id/tvRoterApfel"
                app:srcCompat="@drawable/gurke"
                android:backgroundTint="@color/g200"
                tools:ignore="NotSibling" />

            <TextView
                android:id="@+id/tvGurke"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageButtonGurkeChanger"
                android:text="Salatgurke"
                android:textColor="@color/g200"
                android:layout_centerHorizontal="true"/>

            <ImageButton
                android:id="@+id/imageButtonSechserEierChanger"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_centerHorizontal="true"
                android:scaleType="fitCenter"
                android:layout_below="@id/tvGurke"
                app:srcCompat="@drawable/sechser_eier"
                android:backgroundTint="@color/g200"
                tools:ignore="NotSibling" />

            <TextView
                android:id="@+id/tvSechserPackungEier"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageButtonSechserEierChanger"
                android:text="sechser Packung Eier"
                android:textColor="@color/g200"
                android:layout_centerHorizontal="true"/>

            <ImageButton
                android:id="@+id/imageButtonZehnerEierChanger"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_centerHorizontal="true"
                android:scaleType="fitCenter"
                android:layout_below="@id/tvSechserPackungEier"
                app:srcCompat="@drawable/zehner_eier"
                android:backgroundTint="@color/g200"
                tools:ignore="NotSibling" />

            <TextView
                android:id="@+id/tvZehnerPackungEier"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageButtonZehnerEierChanger"
                android:text="zehner Packung Eier"
                android:textColor="@color/g200"
                android:layout_centerHorizontal="true"/>

            <ImageButton
                android:id="@+id/imageButtonScheibenkaeseChanger"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_centerHorizontal="true"
                android:scaleType="fitCenter"
                android:layout_below="@id/tvZehnerPackungEier"
                app:srcCompat="@drawable/kaesescheibe"
                android:backgroundTint="@color/g200"
                tools:ignore="NotSibling" />

            <TextView
                android:id="@+id/tvScheibenkaese"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageButtonScheibenkaeseChanger"
                android:text="Scheibenk채se"
                android:textColor="@color/g200"
                android:layout_centerHorizontal="true"/>

            <ImageButton
                android:id="@+id/imageButtonHartkaeseChanger"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_centerHorizontal="true"
                android:scaleType="fitCenter"
                android:layout_below="@id/tvScheibenkaese"
                app:srcCompat="@drawable/hartkaese"
                android:backgroundTint="@color/g200"
                tools:ignore="NotSibling" />

            <TextView
                android:id="@+id/tvHartkaese"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageButtonHartkaeseChanger"
                android:text="Hartk채se"
                android:textColor="@color/g200"
                android:layout_centerHorizontal="true"/>

            <ImageButton
                android:id="@+id/imageButtonStreichkaeseChanger"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_centerHorizontal="true"
                android:scaleType="fitCenter"
                android:layout_below="@id/tvHartkaese"
                app:srcCompat="@drawable/streichkaese"
                android:backgroundTint="@color/g200"
                tools:ignore="NotSibling" />

            <TextView
                android:id="@+id/tvStreichkaese"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageButtonStreichkaeseChanger"
                android:text="Streichk채se"
                android:paddingBottom="10dp"
                android:textColor="@color/g200"
                android:layout_centerHorizontal="true"/>


        </RelativeLayout>

    </ScrollView>



</RelativeLayout>