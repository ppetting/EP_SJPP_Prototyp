<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/RelativeLayoutDeletePageEbeneONE"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/g700"
    tools:context=".Menuebereich.UserOverviewActivity"
    android:orientation="vertical">

    <include
        android:id="@+id/toolbarMAIN"
        layout="@layout/toolbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButtonPictureChangeMenue"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:backgroundTint="@color/g200"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/replace"
        tools:ignore="NotSibling" />

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/LinearLayoutDeletePage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/g700"
            android:orientation="vertical">
        </LinearLayout>

    </ScrollView>

</LinearLayout>